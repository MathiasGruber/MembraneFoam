FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    symm
    {
        type            symmetryPlane;
    }
    inletTop
    {
        type            zeroGradient;
    }
    outletTop
    {
        type            fixedValue;
        value           uniform 0;
    }
    outletBottom
    {
        type            fixedValue;
        value           uniform 0;
    }
    inletBottom
    {
        type            zeroGradient;
    }
    tempWall
    {
        type            zeroGradient;
    }
    inletWall
    {
        type            zeroGradient;
    }
    chamberWall
    {
        type            zeroGradient;
    }
    Membrane
    {
        type            zeroGradient;
    }
}


